{
  "name": "spotify-controller-server",
  "version": "1.0.0",
  "description": "Backend server for DJ Forge Controller",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "npx tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testNamePattern=\"Real API Integration Tests\" --verbose --no-coverage --runInBand",
    "test:integration:expensive": "RUN_EXPENSIVE_TESTS=true jest --testNamePattern=\"Real API Integration Tests\" --verbose --no-coverage --runInBand"
  },
  "dependencies": {
    "@google/genai": "^1.9.0",
    "@sentry/node": "^9.42.0",
    "@sentry/profiling-node": "^9.42.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/redis": "^4.0.10",
    "@types/session-file-store": "^1.2.5",
    "@types/winston": "^2.4.4",
    "axios": "^1.7.2",
    "connect-redis": "^9.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "openai": "^5.12.0",
    "redis": "^5.6.0",
    "session-file-store": "^1.5.0",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.5",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.14.9",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "jest": "^30.0.5",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  }
}
